<body>
<form>
  <div class="form-group">
    <label for="exampleInputEmail1">Nombre</label>
    <input type="text" class="form-control" id="name" placeholder="Ingrese Nombre">
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Email</label>
    <input type="text" class="form-control" id="email" placeholder="Ingrese Email">
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Fecha de Nacimiento</label>
    <input type="text" class="form-control" id="fecha" placeholder="Ingrese Fecha de nacimiento">
    <small id="" class="form-text text-muted">En el formato dd/mm/aaaa</small>
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Pais</label>
    <input type="text" class="form-control" id="country" placeholder="Ingrese Pais">
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Equipo</label>
    <input type="text" class="form-control" id="equipo" placeholder="Ingrese Equipo">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>

